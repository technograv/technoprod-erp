+    /**
+     * Applique les rÃ¨gles d'exclusion gÃ©ographique selon la hiÃ©rarchie :
+     * commune < code postal < EPCI < dÃ©partement < rÃ©gion
+     * 
+     * Quand une zone plus petite est incluse dans un secteur, 
+     * alors cette mÃªme zone est exclue des ensembles plus grands dans les autres secteurs.
+     */
+    private function appliquerReglesExclusionGeographique(AttributionSecteur $nouvelleAttribution, EntityManagerInterface $entityManager): void
+    {
+        try {
+            $division = $nouvelleAttribution->getDivisionAdministrative();
+            $secteurCible = $nouvelleAttribution->getSecteur();
+            $typeCritere = $nouvelleAttribution->getTypeCritere();
+            
+            error_log("ğŸ”„ Application rÃ¨gles exclusion pour $typeCritere dans secteur " . $secteurCible->getNomSecteur());
+        
+        // Traitement spÃ©cial pour les codes postaux
+        if ($typeCritere === 'code_postal') {
+            $this->appliquerExclusionsCodePostal($nouvelleAttribution, $entityManager);
+            return;
+        }
+        
+        // DÃ©finir la hiÃ©rarchie gÃ©ographique (ordre croissant de spÃ©cificitÃ©)
+        $hierarchie = [
+            'region' => 1,
+            'departement' => 2, 
+            'epci' => 3,
+            'code_postal' => 4,
+            'commune' => 5
+        ];
+        
+        if (!isset($hierarchie[$typeCritere])) {
+            return; // Type non gÃ©rÃ© par les rÃ¨gles d'exclusion
+        }
+        
+        $prioriteNouvelle = $hierarchie[$typeCritere];
+        
+        // RÃ©cupÃ©rer toutes les attributions qui pourraient inclure cette division administrative
+        // selon la hiÃ©rarchie gÃ©ographique
+        $attributionsExistantes = $this->rechercherAttributionsInclusives($division, $secteurCible, $entityManager);
+        
+        foreach ($attributionsExistantes as $attributionExistante) {
+            $typeExistant = $attributionExistante->getTypeCritere();
+            
+            if (!isset($hierarchie[$typeExistant])) {
+                continue; // Type non gÃ©rÃ©
+            }
+            
+            $prioriteExistante = $hierarchie[$typeExistant];
+            
+            // Si la nouvelle attribution est plus spÃ©cifique (prioritÃ© plus Ã©levÃ©e)
+            // alors crÃ©er une exclusion dans l'attribution moins spÃ©cifique
+            if ($prioriteNouvelle > $prioriteExistante) {
+                // VÃ©rifier si la nouvelle zone est incluse dans la zone existante
+                if ($this->estZoneIncluse($nouvelleAttribution, $attributionExistante, $division)) {
+                    
+                    // VÃ©rifier si l'exclusion n'existe pas dÃ©jÃ 
+                    $exclusionExistante = $entityManager->getRepository(ExclusionSecteur::class)
+                        ->findOneBy([
+                            'attributionSecteur' => $attributionExistante,
+                            'divisionAdministrative' => $division
+                        ]);
+                    
+                    if (!$exclusionExistante) {
+                        // CrÃ©er une exclusion au lieu de supprimer l'attribution
+                        $exclusion = new ExclusionSecteur();
+                        $exclusion->setAttributionSecteur($attributionExistante);
+                        $exclusion->setDivisionAdministrative($division);
+                        $exclusion->setTypeExclusion($typeCritere);
+                        $exclusion->setValeurExclusion($nouvelleAttribution->getValeurCritere());
+                        $exclusion->setMotif("Zone plus spÃ©cifique assignÃ©e au secteur '{$secteurCible->getNomSecteur()}'");
+                        
+                        $entityManager->persist($exclusion);
+                        
+                        $secteurAffecte = $attributionExistante->getSecteur()->getNomSecteur();
+                        $typeExistant = $attributionExistante->getTypeCritere();
+                        $valeurExistante = $attributionExistante->getValeurCritere();
+                        
+                        error_log("ğŸ¯ Exclusion gÃ©ographique : CrÃ©ation exclusion $typeCritere '{$nouvelleAttribution->getValeurCritere()}' dans $typeExistant '$valeurExistante' du secteur '$secteurAffecte'");
+                    }
+                }
+            }
+        }
+        
+        // CAS INVERSE : Si on ajoute un EPCI/dÃ©partement/rÃ©gion, vÃ©rifier s'il y a des communes 
+        // dÃ©jÃ  attribuÃ©es spÃ©cifiquement Ã  d'autres secteurs
+        if (in_array($typeCritere, ['epci', 'departement', 'region', 'code_postal'])) {
+            $this->appliquerExclusionsInverses($nouvelleAttribution, $entityManager);
+        }
+        
+        } catch (\Exception $e) {
+            error_log("âŒ Erreur dans appliquerReglesExclusionGeographique: " . $e->getMessage());
+            error_log("âŒ Stack trace exclusion: " . $e->getTraceAsString());
+            throw $e; // Re-lancer l'exception pour qu'elle soit capturÃ©e par le contrÃ´leur principal
+        }
+    }
