+
+        $data = json_decode($request->getContent(), true);
+
+        // Valider les donnÃ©es
+        $erreurs = $entityManager->getRepository(TypeSecteur::class)->validerDonnees($typeSecteur);
+        if (!empty($erreurs)) {
+            return $this->json(['error' => implode(', ', $erreurs)], 400);
