+                return null;
+            }
+            
+            $data = json_decode($response, true);
+            
+            if (!isset($data['geometry'])) {
+                error_log("âŒ Pas de gÃ©omÃ©trie pour commune $nomCommune ($codeInsee)");
+                return null;
+            }
+            
+            // Convertir la gÃ©omÃ©trie GeoJSON en format compatible
+            $boundaries = $this->extractBoundariesFromGeoJSON($data['geometry']);
