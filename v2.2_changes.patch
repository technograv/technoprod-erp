+}
+
+function supprimerGroupe(groupeId) {
+    const row = document.querySelector(`tr[data-groupe-id="${groupeId}"]`);
+    const nomGroupe = row.querySelector('strong').textContent;
+    
+    document.getElementById('messageConfirmation').textContent = 
