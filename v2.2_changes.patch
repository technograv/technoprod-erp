+
+        if (isset($data['nom'])) $typeSecteur->setNom($data['nom']);
+        if (isset($data['description'])) $typeSecteur->setDescription($data['description']);
+        if (isset($data['actif'])) $typeSecteur->setActif($data['actif']);
