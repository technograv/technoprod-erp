+
+        $allLatitudes = [];
+        $allLongitudes = [];
+
+        // Collecter toutes les coordonnÃ©es de toutes les communes
+        foreach ($communesAvecGeometries as $commune) {
+            if (isset($commune['boundaries']) && !empty($commune['boundaries'])) {
