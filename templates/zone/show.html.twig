{% extends 'base.html.twig' %}

{% block title %}Zone{% endblock %}

{% block body %}
    <h1>Zone</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ zone.id }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ zone.codePostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ zone.ville }}</td>
            </tr>
            <tr>
                <th>Departement</th>
                <td>{{ zone.departement }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ zone.region }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ zone.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ zone.longitude }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ zone.createdAt ? zone.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_zone_index') }}">back to list</a>

    <a href="{{ path('app_zone_edit', {'id': zone.id}) }}">edit</a>

    {{ include('zone/_delete_form.html.twig') }}
{% endblock %}
